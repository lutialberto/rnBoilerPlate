{
  "name": "rnboilerplate",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest",
    "prepare": "husky install",
    "android-dev": "SET ENVFILE=.env.dev && react-native run-android",
    "android-qa": "SET ENVFILE=.env.qa && react-native run-android",
    "android-prod": "SET ENVFILE=.env.prod && react-native run-android",
    "android-dev-apk": "SET ENVFILE=.env.dev && cd android && gradlew assembleRelease",
    "android-qa-apk": "SET ENVFILE=.env.qa && cd android && gradlew assembleRelease",
    "android-prod-apk": "SET ENVFILE=.env.prod && cd android && gradlew assembleRelease",
    "android-dev-aab": "SET ENVFILE=.env.dev && cd android && gradlew bundleRelease",
    "android-qa-aab": "SET ENVFILE=.env.qa && cd android && gradlew bundleRelease",
    "android-prod-aab": "SET ENVFILE=.env.prod && cd android && gradlew bundleRelease"
  },
  "dependencies": {
    "react": "18.2.0",
    "react-native": "0.71.0",
    "react-native-config": "^1.4.12",
    "react-native-svg": "^13.7.0",
    "react-native-svg-transformer": "^1.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/preset-env": "^7.14.0",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^3.0.0",
    "@tsconfig/react-native": "^2.0.2",
    "@types/jest": "^29.2.1",
    "@types/react": "^18.0.24",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.2.1",
    "babel-plugin-root-import": "^6.6.0",
    "eslint": "^8.19.0",
    "husky": "^8.0.0",
    "jest": "^29.2.1",
    "metro-react-native-babel-preset": "0.73.5",
    "prettier": "2.8.3",
    "pretty-quick": "^3.1.3",
    "react-test-renderer": "18.2.0",
    "typescript": "4.8.4"
  },
  "jest": {
    "preset": "react-native"
  }
}
